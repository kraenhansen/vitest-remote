{
  "name": "vitest-remote-client",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc --build"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./runner": {
      "node": "./dist/node/runner.js",
      "react-native": {
        "import": "./dist/react-native/runner.js",
        "types": "./dist/react-native/runner.d.ts"
      },
      "browser": "./dist/browser/runner.js",
      "types": "./dist/runner.d.ts"
    }
  },
  "nx": {
    "targets": {
      "build": {
        "cache": true,
        "outputs": ["{projectRoot}/dist"]
      }
    }
  },
  "peerDependencies": {
    "@vitest/runner": "^0.34.6",
    "@vitest/browser": "^0.34.6"
  }
}